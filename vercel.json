{
  "builds": [
    {
      "src": "fastapi_vercel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "fastapi_vercel.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "LANGSMITH_API_KEY": "@langsmith_api_key",
    "LANGSMITH_PROJECT": "@langsmith_project"
  },
  "functions": {
    "fastapi_vercel.py": {
      "maxDuration": 30
    }
  }
}
