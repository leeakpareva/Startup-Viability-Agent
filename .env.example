# NAVADA Environment Configuration Template
# Copy this file to .env and fill in your actual API keys

# Required - OpenAI API Key for GPT models
OPENAI_API_KEY=your_openai_api_key_here

# LangSmith Tracing Configuration (Optional - for conversation tracking)
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=navada-startup-agent

# Internet Search Configuration (Optional - for real-time market intelligence)
SEARCH_API_KEY=your_brave_search_api_key_here

# Text-to-Speech Configuration (Optional - for voice responses)
TTS_PROMPT_ID=your_tts_prompt_id_here

# LangChain Database Configuration (Optional - for RAG capabilities)
LANGCHAIN_DATABASE_ID=your_langchain_database_id_here
